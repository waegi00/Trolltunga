@model ICollection<Trolltunga.Models.Message>
@foreach (var message in Model)
{
    <div class="media" data-id="@message.ApplicationUser.Id" >
        <div class="media-left">
            <a href="#">
                <img class="media-object" src="http://via.placeholder.com/64x64">
            </a>
        </div>
        <div class="media-body">
            <h4 class="media-heading">@message.ApplicationUser.UserName</h4>
            @message.Content
        </div>
    </div>
}
